{
    "files.exclude": {
        "coverage/": true,
        ".vs": true,
        "node_modules/": true,
        "LICENSE": true,
        "dist": true,
        "public": true,
        "**/*.js": {
            "when": "$(basename).ts"
        }
    },
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true,
    "editor.tabSize": 4,
    "git.ignoreLimitWarning": true,
    "typescript.tsdk": "./node_modules/typescript/lib",
    "cSpell.words": [
        "Apicontroller",
        "Karthik",
        "Konkula",
        "Truthy",
        "binded",
        "bindmodel",
        "dicontainer",
        "dinoloop",
        "eval",
        "exceptionfilter",
        "expresswares",
        "exwares",
        "fotos",
        "getall",
        "gmail",
        "httpbody",
        "httpstatus",
        "httpverb",
        "idino",
        "inversify",
        "ivalidator",
        "jsdoc",
        "kjhtml",
        "kkonkula",
        "middlewares",
        "middlwares",
        "multiline",
        "mware",
        "mwares",
        "nospace",
        "npmignore",
        "onespace",
        "paren",
        "parens",
        "preblock",
        "preprocessors",
        "quotemark",
        "ruri",
        "sampledata",
        "sampleuri",
        "setall",
        "singleline",
        "superbase",
        "testerror",
        "testuri",
        "typeof",
        "validators"
    ],
    "cSpell.enabledLanguageIds": [
        "javascript",
        "json",
        "jsonc",
        "typescript",
        "yml"
    ]
}
